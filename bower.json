{
    "name": "me.tomsen.restapi",
    "version": "1.1.0",
    "description": "A component for wroking with my Java restapi based on tokens. Allows to execute authenticated http queries.",
    "main": ["restapi.module.js", "restapi.provider.js", "cryptojs.factory.js"],
    "keywords": [
        "tomsen.me",
        "restapi",
        "angular"
    ],
    "authors": [
        "Evgeny Tomsen"
    ],
    "license": "MIT",
    "homepage": "www.restapi.tomsen.me",
    "dependencies": {
        "angular": "~1.3.0",
        "angular-local-store": "~0.2.3",
        "cryptojslib": "~3.1.2"
    },
    "overrides": {
        "cryptojslib": {
            "main": [
                "components/core.js",
                "components/enc-base64.js",
                "components/sha256.js"
            ]
        }
    },
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ]
}
